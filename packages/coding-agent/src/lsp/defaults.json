{
	"typescript-language-server": {
		"command": "typescript-language-server",
		"args": ["--stdio"],
		"languages": ["typescript", "typescriptreact", "javascript", "javascriptreact"],
		"installer": {
			"kind": "npm",
			"package": "typescript-language-server"
		}
	},
	"vscode-json-language-server": {
		"command": "vscode-json-language-server",
		"args": ["--stdio"],
		"languages": ["json", "jsonc"],
		"installer": {
			"kind": "npm",
			"package": "vscode-langservers-extracted"
		}
	},
	"vscode-css-language-server": {
		"command": "vscode-css-language-server",
		"args": ["--stdio"],
		"languages": ["css", "scss", "sass", "less"],
		"installer": {
			"kind": "npm",
			"package": "vscode-langservers-extracted"
		}
	},
	"vscode-html-language-server": {
		"command": "vscode-html-language-server",
		"args": ["--stdio"],
		"languages": ["html"],
		"installer": {
			"kind": "npm",
			"package": "vscode-langservers-extracted"
		}
	},
	"pyright": {
		"command": "pyright-langserver",
		"args": ["--stdio"],
		"languages": ["python"],
		"installer": {
			"kind": "pip",
			"package": "pyright"
		}
	},
	"rust-analyzer": {
		"command": "rust-analyzer",
		"languages": ["rust"],
		"installer": {
			"kind": "unsupported",
			"remediation": "Install rust-analyzer via rustup (`rustup component add rust-analyzer`) or your package manager."
		}
	},
	"gopls": {
		"command": "gopls",
		"languages": ["go"],
		"installer": {
			"kind": "unsupported",
			"remediation": "Install gopls with `go install golang.org/x/tools/gopls@latest`."
		}
	}
}
